@model FrontendReport.ViewModels.ReportViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>Report</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Email</th>
            <th>Gender</th>
            <th>IP Address</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Reports) {
        <tr>

            <td>
                @item.ID
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Surname
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Gender
            </td>
            <td>
                @item.IPAddress
            </td>
        </tr>
}
    </tbody>
</table>
